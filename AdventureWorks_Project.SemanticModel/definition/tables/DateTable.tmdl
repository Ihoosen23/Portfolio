	partition DateTable = calculated
		mode: import
		source =
				
				VAR MinDate = MIN( Query1[OrderDate] ) // Finds the earliest date in your data
				VAR MaxDate = MAX( Query1[OrderDate] ) // Finds the latest date in your data
				RETURN
				ADDCOLUMNS(
				    CALENDAR( MinDate, MaxDate ), // Creates a table with every date between Min and Max
				    "Year", YEAR( [Date] ),
				    "Month Number", FORMAT( [Date], "MM" ),
				    "Month Name", FORMAT( [Date], "MMMM" ),
				    "Quarter", "Q" & FORMAT( [Date], "Q" ),
				    "Year Quarter", FORMAT( [Date], "YYYY" ) & " Q" & FORMAT( [Date], "Q" )
				)

